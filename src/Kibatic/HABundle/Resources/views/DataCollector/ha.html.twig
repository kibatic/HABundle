{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img src="data:image/png;base64," alt="HA" />
        <span class="sf-toolbar-status">{{ collector.data.host }}</span>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Server</b>
            <span>{{ collector.data.host }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>HTTP Host</b>
            <span>{{ collector.data.http_host }}</span>
        </div>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    <span class="label">
    <span class="icon">
        <img src="data:image/png;base64," alt="HA" />
    </span>
    <strong>High availability</strong>
</span>
{% endblock %}

{% block panel %}
    <h2>High availability</h2>
    <table>
        <tr>
            <th>Label</th>
            <th>Value</th>
        </tr>
        {% for key, data in collector.data %}
        <tr>
            <td>{{ key }}</td>
            <td>{{ dump(data) }}</td>
        </tr>
        {% endfor %}
    </table>
{% endblock %}
